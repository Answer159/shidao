AA,0CAAA,KAAA;AAFA,WAAA;;AAIA,UAAA,WAAA,CAAA,YAAA;AACA,YAAA,KAAA;;AACA,gBAAA,KAAA,IAAA,CAAA,EAAA;AACA,cAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,OAAA;AACA;AACA,WALA,EAKA,IALA,CAAA;AAMA,SA1BA,CA2BA;;;AACA,YAAA,GAAA,CAAA,IAAA,CAAA,GAAA,IAAA,MAAA,EAAA;AACA,UAAA,KAAA,CAAA,QAAA,CAAA;AACA,YAAA,IAAA,EAAA,OADA;AAEA,YAAA,OAAA,EAAA;AAFA,WAAA;AAIA;AACA,OAlCA;AAmCA;AAtCA;AATA,CAAA","sourcesContent":["<template>\n  <div class=\"login\">\n    <div style=\"height:100%\">\n    <div class=\"title item\"><h2>密码登录</h2></div>\n    <div class=\"item\" ><input class=\"useraccount\" type=\"text\" v-model=\"useraccount\" placeholder=\"请输入账号\"></input></div>\n    <div class=\" item\"><input class=\"useraccount\" type=\"password\"  v-model=\"password\" placeholder=\"请输入密码\"></input></div>\n    <div class=\"item\"><el-button class=\"button\" type=\"primary\" plain @click=\"LoginClick\" :loading=\"load\"><span style=\"font-size:var(--font-size-big)\">登录</span></el-button></div>\n    <div class=\"item\">没有账号？<router-link style=\"color:var(--color-high-text)\" to=\"/register\" >立即注册</router-link></div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport {Login} from '../../../network/login'\nexport default {\n  name: 'LoginContant',\n  data() {\n    return{\n      useraccount:\"111\",\n      password:\"123\",\n      load:false\n    }\n  },\n  methods:{\n    LoginClick(){\n      this.load = true;\n      Login(this.useraccount,this.password).then((res)=>{\n        //将按钮置回\n        this.load = false;\n        //发送成功提示并调转到首页\n        if(res.data.msg == \"success\"){\n          let data = res.data.userInfo;\n          this.$store.commit('SetUserState',{\n             login: 1,\n            userId: data.id,\n            userName: data.username,\n            userImg: data.userImg,\n            collectionClass: data.collectionClass,\n            collectionQuestion: data.collectionQuestion\n          })\n          console.log(window.sessionStorage.getItem('collectionClass'));\n          let timer = 2;\n          this.$message({\n            type:\"success\",\n            message:`登录成功，${timer}s后跳转!`\n          })\n           setInterval(()=>{\n            timer--;\n            if(timer == 0){\n                this.$router.push('/home');\n            }\n          },1000)     \n        }\n        //发送失败提示\n        if(res.data.msg == \"fail\"){\n          this.$message({\n            type:\"error\",\n            message:\"账号或密码错误，请重新登录！\"\n          })\n        }\n      })\n    }\n  }\n}\n</script>\n\n<style scoped>\n.login{\n  box-sizing: border-box;\n  width: 100%;\n  margin-top: 45px;\n  padding: 8% 15%;\n  height: 60vh;\n  background-color: white;\n   box-shadow: rgba(149, 157, 165, 0.2) 0px 8px 24px;\n}\n\n.item {\n  text-align: center;\n  width: 100%;\n  padding: 15px 0;\n}\n\n.title {\n  text-align: center;\n}\n\n.useraccount{\n  outline-style: none;\n  box-sizing: border-box;\n  width: 100%;\n  font-size: var(--font-size);\n  padding-left: 20px;\n  height: 55px;\n}\n\n.button{\n  margin-top: 40px;\n  width: 100%;\n height: 55px;\n}\n</style>"],"sourceRoot":"src/views/login/chlidcomponents"}]}