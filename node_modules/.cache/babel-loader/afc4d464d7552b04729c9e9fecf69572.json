AA;AACA,MAAA,IAAA,CAAA,MAAA,CAAA,UAAA,EAAA,QAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA,MAAA,QAAA,CAAA,IAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,CAAA,GAAA,KAAA,SAAA,EAAA;AACA,UAAA,KAAA,CAAA,QAAA,CAAA;AACA,YAAA,IAAA,EAAA,SADA;AAEA,YAAA,OAAA;AAFA,WAAA;;AAIA,UAAA,MAAA,GAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,gBAAA,GAAA,CAAA,IAAA,CAAA,GAAA,KAAA,SAAA,EAAA;AACA,cAAA,KAAA,CAAA,MAAA,CAAA,MAAA,CAAA,QAAA;;AACA,cAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,OAAA;AACA,aAHA,MAGA;AACA,cAAA,KAAA,CAAA,QAAA,CAAA;AACA,gBAAA,IAAA,EAAA,OADA;AAEA,gBAAA,OAAA,EAAA;AAFA,eAAA;AAIA;AACA,WAVA;AAWA,UAAA,UAAA,CAAA,YAAA;AACA,YAAA,KAAA,CAAA,IAAA,GAAA,KAAA;;AACA,YAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,QAAA;AACA,WAHA,EAGA,IAHA,CAAA;AAIA,SApBA,MAqBA;AACA,UAAA,KAAA,CAAA,QAAA,CAAA;AACA,YAAA,IAAA,EAAA,OADA;AAEA,YAAA,OAAA,EAAA,GAAA,CAAA,IAAA,CAAA;AAFA,WAAA;;AAIA,UAAA,KAAA,CAAA,IAAA,GAAA,KAAA;AACA;AACA,OA7BA;AA8BA;AA5CA;AAdA,CAAA","sourcesContent":["<template>\r\n  <div class=\"form\">\r\n    <h1 class=\"headOne\">基础信息</h1>\r\n      <el-form ref=\"form\" class=\"contantOne\" :model=\"userInfo\" label-width=\"20%\" size=\"medium\" label-position=\"left\" label-suffix=\":\">\r\n        <el-form-item class=\"item\" label=\"账号\">\r\n          <el-input v-model=\"userInfo.account\" :readonly=\"true\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item class=\"item\" label=\"密码\" >\r\n          <el-input v-model=\"userInfo.password\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item class=\"item\" label=\"手机号码\" >\r\n          <el-input v-model=\"userInfo.phone\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item class=\"item\" label=\"昵称\" >\r\n          <el-input v-model=\"userInfo.username\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item class=\"item\" label=\"头像\" >\r\n          <av-upload :prop-url=\"userInfo.userImg\" ref=\"RegisterAvUpload\"></av-upload>\r\n        </el-form-item>\r\n        <el-form-item class=\"item\" label=\"性别\">\r\n          <el-select v-model=\"userInfo.sex\" placeholder=\"请选择性别\">\r\n            <el-option label=\"男\" :value=\"1\"></el-option>\r\n            <el-option label=\"女\" :value=\"2\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item class=\"item\" label=\"兴趣领域\">\r\n          <category-select :category=\"userInfo.domainId\" @change=\"change\"></category-select>\r\n        </el-form-item>\r\n        <el-form-item  label=\"个人简介\">\r\n          <el-input type=\"textarea\"  resize=\"none\" v-model=\"userInfo.intro\"></el-input>\r\n        </el-form-item>\r\n        \r\n        <el-button type=\"primary\" @click=\"submitClick\">提交</el-button>\r\n      </el-form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport AvUpload from '../../../components/common/upload/AvUpload.vue';\r\nimport { editUser } from '../../../network/register.js'\r\nimport {Logout} from \"../../../network/login\";\r\nimport CategorySelect from \"../../components/categorySelect\";\r\n\r\nexport default {\r\n  name:\"RegisterForm\",\r\n  data(){\r\n    return{\r\n      userInfo:{\r\n      },\r\n      file:null,\r\n    }   \r\n  },\r\n  components:{\r\n    AvUpload,\r\n    CategorySelect\r\n  },\r\n\r\n  methods:{\r\n    change(data){\r\n      this.userInfo.domainId=data;\r\n      console.log(this.userInfo.domainId);\r\n    },\r\n    submitClick(){\r\n       this.file = this.$refs.RegisterAvUpload.file;\r\n       let data = new FormData();\r\n       if(this.file!==null){\r\n         data.append('picture',this.file);\r\n       }\r\n       let userInfo = JSON.stringify(this.userInfo);\r\n       data.append('userInfo',userInfo)\r\n      console.log(userInfo);\r\n      editUser(data).then((res) =>{\r\n        if(res.data.msg === \"success\"){\r\n          this.$message({\r\n            type:\"success\",\r\n            message:`修改成功!`\r\n          })\r\n          Logout().then((res)=>{\r\n            if(res.data.msg === \"success\"){\r\n              this.$store.commit('Logout');\r\n              this.$router.push('/home');\r\n            }else{\r\n              this.$message({\r\n                type:\"error\",\r\n                message:\"登出失败，请重试！\"\r\n              })\r\n            }\r\n          })\r\n          setTimeout(()=>{\r\n            this.load = false;\r\n            this.$router.push('/login');\r\n          },2000);\r\n        }\r\n        else{\r\n          this.$message({\r\n            type:\"error\",\r\n            message:res.data.msg\r\n          })\r\n          this.load = false;\r\n        }\r\n      })  \r\n    }\r\n\r\n  }\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.form{\r\n  margin-top: 45px;\r\n  box-sizing: border-box;\r\n  width: 100%;\r\n  padding: 15px 30px;\r\n  border: 1px solid var(--color-border);\r\n  background-color: white;\r\n}\r\n\r\n.headOne{\r\n  margin-left: 5%;\r\n  width: 60%;\r\n}\r\n\r\n.contantOne{\r\n  margin-left: 20%;\r\n  width: 60%;\r\n  \r\n}\r\n\r\n.headTwo{\r\n  position: relative;\r\n  left: calc(-20%*5/3);\r\n  margin-left: calc(5%*5/3);\r\n  width: 60%;\r\n}\r\n\r\n\r\n::v-deep .el-input__inner{\r\n     height: 50px;\r\n     font-size: var(--font-size);\r\n }\r\n\r\n::v-deep .el-input{\r\n  width: 60%;\r\n}\r\n\r\n::v-deep .el-form-item__label{\r\n  height: 50px;\r\n  line-height: 50px;\r\n  font-size: var(--font-size);\r\n}\r\n\r\n::v-deep .el-textarea__inner{\r\n  height: 150px;\r\n    text-indent: 2em;\r\n    letter-spacing: 0.1em;\r\n    line-height: 1.6;\r\n}\r\n\r\n</style>"],"sourceRoot":"src/views/updataUserInfo/childComponents"}]}