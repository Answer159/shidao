lt {\n  name: 'TabControl',\n  data() {\n    return {\n      currentIndex: 0\n    }\n\n  },\n  props:{\n    names:{\n      type: Array,\n      default() {\n        return []\n      },\n    },\n     currentItem: {\n        type: Number,\n        default: 0\n      }\n  },\n  methods: {\n    ClickItem(index){ \n      this.$emit('ItemClick',index);\n      this.currentIndex = index;\n    }\n  },\n  watch: {\n    currentItem(val) {\n      this.currentIndex = val\n    },\n    currentIndex(index) {\n      this.$emit('ItemClick',index);\n    },\n  }\n  \n\n}\n</script>\n\n<style scoped>\n  .tab-control {\n    display: flex;\n    height: 44px;\n    line-height: 44px;\n    text-align: center;\n  }\n\n  .tab-item {\n    flex: 1;\n    font-size: 18px;\n    color: #666;\n  }\n\n  .current {\n    color: var(--color-high-text);\n    border-bottom: 2px solid var(--color-high-text);\n    padding: 10px 5px;\n  }\n\n</style>"],"sourceRoot":"src/components/common/tabbar"}]}