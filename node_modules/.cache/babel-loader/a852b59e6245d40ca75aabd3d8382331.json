mplate>\n   <div class=\"recommand\">\n    <div class=\"title\">\n      <span class=\"one\">课程推荐</span>\n    </div>\n    <div class=\"contant\">\n      <class-card v-if=\"showFlag\" class=\"item\" v-for=\"item in classes\" :data='item'></class-card>\n      <question-card v-if=\"!showFlag\" class=\"item\"  v-for=\"item in questions\" :data='item'></question-card>\n    </div>\n     <p @click=\"nextCommend\">换一批</p>\n  </div>\n\n</template>\n\n<script>\nimport ClassCard from 'components/contant/class/ClassCard.vue';\nimport QuestionCard from \"../../../components/contant/question/questionCard\";\nimport {recommendClass, recommendQuestion} from \"../../../network/classDetail\";\nexport default {\n  name: \"RecommandClass\",\n  props:{\n    classes: null,\n    questions: null,\n    flag:0\n    },\n  components: {\n    ClassCard,\n    QuestionCard\n  },\n  computed:{\n    showFlag(){\n      console.log(this.classes);\n      if(this.flag === 0){\n        return true;\n      }\n      else{\n        return false;\n      }\n    }\n  },\n  methods:{\n    nextCommend(){\n      if(this.flag === 0){\n        recommendClass().then((res=>{\n          this.classes = res.data.classVos;\n        }))\n\n      }\n      else if(this.flag === 1){\n        recommendQuestion().then((res)=>{\n          this.questions = res.data.questionVos;\n        })\n      }\n    }\n  }\n\n}\n</script>\n\n<style scoped>\n.recommand {\n  width: 90%;\n  margin: 30px 0;\n  margin-left: 10%;\n  \n}\n\n.title {\n   text-align: center;\n}\n.one{\n  display: block;\n  color: #000;\n  font-size: var(--font-size-very-big);\n  font-weight:normal;\n  letter-spacing: 2px;\n}\n\n.contant {\n  display: -webkit-flex;\n  display: flex;\n  flex-direction: column;\n  \n}\n\n.item {\n  margin-top: 30px;\n}\n</style>"],"sourceRoot":"src/views/searchResult/childComponents"}]}