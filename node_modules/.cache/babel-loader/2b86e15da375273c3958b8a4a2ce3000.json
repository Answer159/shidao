.log(res);\r\n      this.$refs.tops.execute();\r\n    })\r\n    \r\n  }\r\n  \r\n\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.navbar {\r\n  height: 80px;\r\n  width: 100%;\r\n  background-color: black;\r\n}\r\n\r\n.bar {\r\n  margin-top: 10px;\r\n}\r\n\r\n.contant {\r\n  margin-top: 45px;\r\n}\r\n\r\n</style>"],"sourceRoot":"src/views/classDetail"}]}