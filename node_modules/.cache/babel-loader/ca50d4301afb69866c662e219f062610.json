CAAA;AAPA,WAAA;;AASA,UAAA,OAAA,CAAA,GAAA,CAAA,GAAA,CAAA,IAAA,CAAA,QAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,MAAA,CAAA,cAAA,CAAA,OAAA,CAAA,YAAA,CAAA;AACA,cAAA,KAAA,GAAA,CAAA;;AACA,UAAA,KAAA,CAAA,QAAA,CAAA;AACA,YAAA,IAAA,EAAA,SADA;AAEA,YAAA,OAAA,0CAAA,KAAA;AAFA,WAAA;;AAIA,UAAA,WAAA,CAAA,YAAA;AACA,YAAA,KAAA;;AACA,gBAAA,KAAA,IAAA,CAAA,EAAA;AACA,cAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,OAAA;AACA;AACA,WALA,EAKA,IALA,CAAA;AAMA,SA5BA,CA6BA;;;AACA,YAAA,GAAA,CAAA,IAAA,CAAA,GAAA,IAAA,MAAA,EAAA;AACA,UAAA,KAAA,CAAA,QAAA,CAAA;AACA,YAAA,IAAA,EAAA,OADA;AAEA,YAAA,OAAA,EAAA;AAFA,WAAA;AAIA;AACA,OApCA;AAqCA;AAxCA;AATA,CAAA","sourcesContent":["<template>\r\n  <div class=\"login\">\r\n    <div style=\"height:100%\">\r\n    <div class=\"title item\"><h2>密码登录</h2></div>\r\n    <div class=\"item\" ><input class=\"useraccount\" type=\"text\" v-model=\"useraccount\" placeholder=\"请输入账号\"></input></div>\r\n    <div class=\" item\"><input class=\"useraccount\" type=\"password\"  v-model=\"password\" placeholder=\"请输入密码\"></input></div>\r\n    <div class=\"item\"><el-button class=\"button\" type=\"primary\" plain @click=\"LoginClick\" :loading=\"load\"><span style=\"font-size:var(--font-size-big)\">登录</span></el-button></div>\r\n    <div class=\"item\">没有账号？<router-link style=\"color:var(--color-high-text)\" to=\"/register\" >立即注册</router-link></div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {Login} from '../../../network/login'\r\nexport default {\r\n  name: 'LoginContant',\r\n  data() {\r\n    return{\r\n      useraccount:\"111\",\r\n      password:\"123\",\r\n      load:false\r\n    }\r\n  },\r\n  methods:{\r\n    LoginClick(){\r\n      this.load = true;\r\n      Login(this.useraccount,this.password).then((res)=>{\r\n        //将按钮置回\r\n        this.load = false;\r\n        //发送成功提示并调转到首页\r\n        if(res.data.msg == \"success\"){\r\n          let data = res.data.userInfo;\r\n          this.$store.commit('SetUserState',{\r\n             login: 1,\r\n            userId: data.id,\r\n            userName: data.username,\r\n            domainText: data.domainText,\r\n            userImg: data.userImg,\r\n            collectionClass: data.collectionClass,\r\n            collectionQuestion: data.collectionQuestion\r\n          })\r\n          console.log(res.data.userInfo)\r\n          console.log(window.sessionStorage.getItem('domainText'));\r\n          let timer = 2;\r\n          this.$message({\r\n            type:\"success\",\r\n            message:`登录成功，${timer}s后跳转!`\r\n          })\r\n           setInterval(()=>{\r\n            timer--;\r\n            if(timer == 0){\r\n                this.$router.push('/home');\r\n            }\r\n          },1000)     \r\n        }\r\n        //发送失败提示\r\n        if(res.data.msg == \"fail\"){\r\n          this.$message({\r\n            type:\"error\",\r\n            message:\"账号或密码错误，请重新登录！\"\r\n          })\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.login{\r\n  box-sizing: border-box;\r\n  width: 100%;\r\n  margin-top: 45px;\r\n  padding: 8% 15%;\r\n  height: 60vh;\r\n  background-color: white;\r\n   box-shadow: rgba(149, 157, 165, 0.2) 0px 8px 24px;\r\n}\r\n\r\n.item {\r\n  text-align: center;\r\n  width: 100%;\r\n  padding: 15px 0;\r\n}\r\n\r\n.title {\r\n  text-align: center;\r\n}\r\n\r\n.useraccount{\r\n  outline-style: none;\r\n  box-sizing: border-box;\r\n  width: 100%;\r\n  font-size: var(--font-size);\r\n  padding-left: 20px;\r\n  height: 55px;\r\n}\r\n\r\n.button{\r\n  margin-top: 40px;\r\n  width: 100%;\r\n height: 55px;\r\n}\r\n</style>"],"sourceRoot":"src/views/login/chlidcomponents"}]}