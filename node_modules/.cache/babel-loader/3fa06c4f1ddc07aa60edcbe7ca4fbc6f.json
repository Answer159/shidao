AA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,QAAA,CAAA;AACA,UAAA,IAAA,EAAA,SADA;AAEA,UAAA,OAAA;AAFA,SAAA;AAIA,OALA;AAMA,KARA;AASA,IAAA,OATA,qBASA;AACA,WAAA,EAAA,GAAA,IAAA;AACA,KAXA;AAYA,IAAA,IAZA,kBAYA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA,YAAA;AAAA,QAAA,KAAA,EAAA;AAAA,UAAA,EAAA,EAAA,KAAA,IAAA,CAAA;AAAA;AAAA,OAAA;AACA;AAdA;AA5BA,CAAA","sourcesContent":["<template>\r\n\r\n  <div class=\"detail\"> \r\n    <collect-button v-if=\"ok\" :classId=\"data.id\"></collect-button>\r\n    <div class=\"img\">\r\n      <el-carousel trigger=\"click\">\r\n        <el-carousel-item v-for=\"item in imgPath\" :key=\"item\">\r\n          <div class=\"imgitem\"><img :src=\"item\" alt=\"\"></div>\r\n        </el-carousel-item>\r\n      </el-carousel>\r\n    </div>\r\n    <div class=\"contant\">\r\n      <div class=\"title title-text\">{{data.title}}</div>\r\n      <div class=\"score\">\r\n        <el-rate\r\n          v-model=\"data.score.toString()\"\r\n          disabled\r\n          show-score\r\n          text-color=\"#ff9900\"\r\n          :score-template=\"data.score.toString()\">\r\n        </el-rate>\r\n      </div>\r\n      <div class=\"teacher\">参考价格：￥{{data.price}}</div>\r\n      <div class=\"teacher\">参考时长：{{data.suggestTime}}</div>\r\n      <div class=\"teacher\">课程领域：{{data.cateText}}</div>\r\n      <div class=\"teacher\">教师：{{user.username}}</div>\r\n      <div class=\"button\" v-if=\"isChange\"> <el-button type=\"edit\" plain @click=\"edit\">修改课程</el-button></div>\r\n      <div class=\"button\"> <el-button type=\"primary\" plain @click=\"want\">立即购买</el-button></div>\r\n    </div>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport CollectButton from '../../../components/contant/collect/CollectButton.vue'\r\nimport {want} from \"../../../network/Order\";\r\nexport default {\r\n  name: \"ClassDetailTop\",\r\n  props: {\r\n    data:null,\r\n    user:null,\r\n    imgPath:null,\r\n    canChange:false,\r\n  },\r\n  data(){\r\n    return{\r\n      ok:false,\r\n      isCollect:true\r\n    }\r\n  },\r\n  components:{\r\n    CollectButton\r\n  },\r\n  computed:{\r\n    isChange(){\r\n      console.log(this.canChange);\r\n      if(this.canChange === \"0\" ||this.canChange === 0){\r\n        return false;\r\n      }\r\n      else if(this.canChange === \"1\" ||this.canChange === 1){\r\n        return true;\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    want(){\r\n      want(this.data.id).then((res)=>{\r\n        this.$message({\r\n          type:\"success\",\r\n          message:`创建课程订单成功!`\r\n        })\r\n      })\r\n    },\r\n    execute(){\r\n      this.ok=true;\r\n    },\r\n    edit(){\r\n      this.$router.push({path:'/editClass',query:{id:this.data.id}})\r\n    }\r\n  }\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n.detail{\r\n  display: flex;\r\n  margin-top: 45px;\r\n  padding: 20px 20px;\r\n  background-color: white;\r\n    box-shadow: rgba(149, 157, 165, 0.2) 0px 8px 24px;\r\n}\r\n\r\n.img {\r\n  width: 37.5%;\r\n}\r\n\r\n.imgitem{\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  width: 100%;\r\n  height: 100%;\r\n}\r\n\r\nimg{\r\n  \r\n  width: 100%;\r\n  overflow: hidden;\r\n  \r\n}\r\n\r\n.contant {\r\n  box-sizing: border-box;\r\n  width: 62.5%;\r\n  padding-left: 30px;\r\n}\r\n\r\n.teacher{\r\n  margin-top: 8px;\r\n}\r\n.button{\r\n  margin-top: 8px;\r\n}\r\n</style>"],"sourceRoot":"src/views/classDetail/childComponents"}]}