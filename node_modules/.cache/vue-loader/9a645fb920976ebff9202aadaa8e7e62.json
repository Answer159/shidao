l-input>\n        </el-form-item>\n        <el-form-item class=\"item\" label=\"昵称\" >\n          <el-input v-model=\"userInfo.username\"></el-input>\n        </el-form-item>\n        <el-form-item class=\"item\" label=\"头像\" >\n          <av-upload ref=\"RegisterAvUpload\"></av-upload>\n        </el-form-item>\n        <el-form-item class=\"item\" label=\"性别\">\n          <el-select v-model=\"userInfo.sex\" placeholder=\"请选择性别\">\n            <el-option label=\"男\" :value=\"1\"></el-option>\n            <el-option label=\"女\" :value=\"2\"></el-option>\n          </el-select>\n        </el-form-item>\n        <el-form-item class=\"item\" label=\"兴趣领域\">\n          <category-select v-model=\"userInfo.domainId\" @change=\"change\"></category-select>\n<!--          <el-select v-model=\"userInfo.domainId\" placeholder=\"请选择课程领域\">-->\n<!--            <el-option label=\"区域一\" :value=\"1\"></el-option>-->\n<!--            <el-option label=\"区域二\" :value=\"2\"></el-option>-->\n<!--          </el-select>-->\n        </el-form-item>\n        <el-form-item  label=\"个人简介\">\n          <el-input type=\"textarea\"  resize=\"none\" v-model=\"userInfo.selfIntro\" :maxlength=\"200\" :show-word-limit=\"true\"></el-input>\n        </el-form-item>\n        \n        <el-button type=\"primary\" @click=\"submitClick\" :loading=\"load\">提交</el-button>\n      </el-form>\n  </div>\n</template>\n\n<script>\nimport AvUpload from '../../../components/common/upload/AvUpload.vue';\nimport { submit } from 'network/register.js'\nimport categorySelect from \"../../components/categorySelect\";\nexport default {\n  name:\"RegisterForm\",\n  data(){\n    return{\n      load:false,\n      userInfo:{\n        id:0,\n        graphId:0,\n        collectionClass:\"\",\n        collectionQuestion:\"\",\n        username:\"\",\n        account:\"\",\n        password:\"\",\n        phone:\"\",\n        sex:1,\n        selfIntro:\"\",\n        domainId:1\n      },\n      file:null,\n    }   \n  },\n  components:{\n    AvUpload,\n    categorySelect\n  },\n\n  methods:{\n    change(data){\n      this.userInfo.domainId=data;\n    },\n    submitClick(){\n      this.load = true;\n       this.file = this.$refs.RegisterAvUpload.file;\n       let data = new FormData();\n       data.append('picture',this.file) ;\n       console.log(\"picture is:\");\n      console.log(this.file);\n       let userInfo = JSON.stringify(this.userInfo);\n       data.append('userInfo',userInfo)\n      submit(data).then((res) =>{\n        if(res.data.msg == \"注册成功\"){\n          this.$message({\n            type:\"success\",\n            message:`注册成功，2s后跳转登录页面!`\n          })\n\n          setTimeout(()=>{\n            this.load = false;\n            this.$router.push('/login');          \n          },2000);         \n        }\n        if(res.data.msg == \"用户名已被占用，不能使用\"){\n          this.$message({\n            type:\"error\",\n            message:`账号已被占用，不能使用!`\n          })\n          this.load = false;         \n        }  \n      })  \n    }\n\n  }\n\n}\n</script>\n\n<style scoped>\n.form{\n  margin-top: 45px;\n  box-sizing: border-box;\n  width: 100%;\n  padding: 15px 30px;\n  border: 1px solid var(--color-border);\n  background-color: white;\n}\n\n.headOne{\n  margin-left: 5%;\n  width: 60%;\n}\n\n.contantOne{\n  margin-left: 20%;\n  width: 60%;\n  \n}\n\n.headTwo{\n  position: relative;\n  left: calc(-20%*5/3);\n  margin-left: calc(5%*5/3);\n  width: 60%;\n}\n\n\n::v-deep .el-input__inner{\n     height: 50px;\n     font-size: var(--font-size);\n }\n\n::v-deep .el-input{\n  width: 60%;\n}\n\n::v-deep .el-form-item__label{\n  height: 50px;\n  line-height: 50px;\n  font-size: var(--font-size);\n}\n\n::v-deep .el-textarea__inner{\n  height: 150px;\n    text-indent: 2em;\n    letter-spacing: 0.1em;\n    line-height: 1.6;\n}\n\n</style>"]}]}